@model Address

<div class="form-group">
    @Html.MyLabelFor(m => m.Street)
    @Html.MyDataBoundTextBoxFor(m => m.Street, "value: street")
</div>
<div class="form-group">
    @Html.MyLabelFor(m => m.Zip)
    @Html.MyDataBoundTextBoxFor(m => m.Zip, "value: zip")
</div>
<div class="form-group">
    @Html.MyLabelFor(m => m.City)
    @Html.MyDataBoundTextBoxFor(m => m.City, "value: city")
</div>
<div class="form-group">
    @Html.MyLabelFor(m => m.CountryCode)
    @Html.MyDataBoundTextBoxFor(m => m.CountryCode, "value: countryCode")
</div>
